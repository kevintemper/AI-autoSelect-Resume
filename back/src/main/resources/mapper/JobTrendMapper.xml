<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.zwtcampuscareerview.repositories.mapper.JobTrendMapper">
    <select id="getJobTrendDataByYear" resultType="com.example.zwtcampuscareerview.models.JobTrendData">
        SELECT *
        FROM job_trend_data
        WHERE year = #{year}
    </select>
</mapper>
